<?xml version="1.0" encoding="UTF-8"?>
<!--Rearranged the Discuss start meeting template-->
<templates id="template" xml:space="preserve">
    <t name="mail.DiscussMeetingsAdvanced"
       t-inherit="mail.DiscussSidebarStartMeeting"
       t-inherit-mode="extension"
       owl="1">
           <xpath expr="//div" position="replace">
               <div class="o_DiscussSidebar_startAMeetingButton w-100 "
                    title="Meeting"
                    t-on-click="onClickStartMeeting">
                   <button class="sidebar-tabs meet-btn__container"
                           data-hotkey="m"> <i
                           class="fa fa-video-camera"
                           style="margin-right:0.4rem"> </i>Meeting</button>
               </div>
           </xpath>
        </t>
</templates>
