<?xml version="1.0" encoding="UTF-8"?>
<!--Template is used for viewing created transfer popup reference value-->
<template id="template" xml:space="preserve">
    <t t-name="TransferRefPopup" owl="1">
        <div class="popup popup-text popup-med stock-transfer-popup">
            <h2 class="p-3">Stock Transfer</h2>
            <div align="center" style="height:60px; margin-top:20px;">
                <td>Stock Transfer Ref :</td>
                <td style="color:#017e84" t-on-click="stock_view"><t t-esc="props.data['name']"/></td>
            </div>
            <div class="footer pb-3">
                <div class="button cancel btn btn-lg btn-secondary"
                     t-on-click="cancel">
                      Ok
                </div>
            </div>
        </div>
    </t>
</template>
