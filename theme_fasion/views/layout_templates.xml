<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- To change title -->
    <template id="theme_fasion_layout" inherit_id="website.layout"
              name="Fashion Layout">
        <xpath expr="//div[@id='wrapwrap']/main" position="after">
            <head>
                <meta charset="UTF-8"/>
                <meta name="viewport"
                      content="width=device-width, initial-scale=1.0"/>
                <title>Fashion</title>
                <link href="https://fonts.googleapis.com/css2?family=Baloo+Tamma+2:wght@400..800&amp;display=swap" rel="stylesheet"/>
                <link href="https://fonts.googleapis.com/css2?family=Lobster+Two:ital,wght@0,400;0,700;1,400;1,700&amp;display=swap" rel="stylesheet"/>
            </head>
        </xpath>
    </template>
</odoo>
